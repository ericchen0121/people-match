          option(value='3') 3
          option(value='4') 4
          option(value='5') 5
          option(value='6') 6
          option(value='7') 7
          option(value='8') 8
          option(value='9') 9
          option(value='10') 10
          option(value='11') 11
          option(value='12') 12
          option(value='13') 13
          option(value='14') 14
          option(value='15') 15
          option(value='16') 16
          option(value='17') 17
        div(class='btn btn-sm btn-default total-score-to-entries-week-all') Go
      div(class='col-md-3')
        h4 Step 3. Rank Entries (optional)
        select(id='rank-entries-sport-select')
          option(value='NFL') NFL Week
        select(id='rank-entries-schedule-select')
          option(value='1') 1
          option(value='2') 2
          option(value='3') 3
          option(value='4') 4
          option(value='5') 5
          option(value='6') 6
          option(value='7') 7
          option(value='8') 8
          option(value='9') 9
          option(value='10') 10
